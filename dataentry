    private void JbaddActionPerformed(java.awt.event.ActionEvent evt) {                                      
       DefaultTableModel mod= (DefaultTableModel) jTable1.getModel();
       mod.setRowCount(0);
        
         String sql= "INSERT INTO dataform(id, firstname, mi, surname, age, course)VALUES(?,?,?,?,?,?)";
             try
        {
        pst = conn.prepareStatement(sql);
     
                pst.setString(1,jtxtid.getText());
                pst.setString(2,jtxtfirstname.getText());
                pst.setString(3,jtxtmi.getText());
                pst.setString(4,jtxtsurname.getText());
                pst.setString(5,jtxtage.getText());
                pst.setString(6,jtxtcourse.getText());
                
               
                 pst.execute();
                 JOptionPane.showMessageDialog(null, "System Updated");
                 rs.close();
                 pst.close();
          
            
            
        }
        catch (HeadlessException | SQLException e)
        {
            JOptionPane.showMessageDialog(null, e);
        
        }
             updateTable();
             
             DefaultTableModel iModel = (DefaultTableModel) jTable1.getModel();
           if (jTable1.getSelectedRow()==-1){
               if(jTable1.getRowCount() == 0){
                   JOptionPane.showMessageDialog(null, "Data Entry Update confirmed","Data Entry System",JOptionPane.OK_OPTION);
               }
           
           
           }
             
    }                                     

