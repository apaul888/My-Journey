    public DataEntry() {
        initComponents();
        
        Object col[] = {"id","firstname","mi","surname","age","course"};
        
        model.setColumnIdentifiers(col);
        jTable1.setModel(model);
        conn = DataEntry.ConnectDb();
        
        updateTable();
    }

